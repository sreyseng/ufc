{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": false,
    "react/destructuring-assignment": false,
    "comma-dangle": ["error", "never"],
    "no-use-before-define": "warn",
    "arrow-parens": "off"
  }
}
